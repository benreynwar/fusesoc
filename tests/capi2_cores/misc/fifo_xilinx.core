CAPI=2:

name : ::fifo_xilinx

filesets:
  main_fileset:
    depend: [generators]

targets:
  default:
    filesets : [main_fileset]
    generate : [fifo_xilinx_gen]
    toplevel : fifo_xilinx
  synth:
    filesets : [main_fileset]
    generate : [fifo_xilinx_gen]
    toplevel : fifo_xilinx

generate:
  fifo_xilinx_gen:
    generator : xilinx_core_generator
    parameters:
      core_name : fifo_generator
      module_name : fifo_xilinx
      version : 13.2
      core_parameters :
        input_data_width : 32
        input_depth : 128
        output_data_width : 32
        output_depth : 128
        data_count_width : 7
        write_data_count_width : 7
        read_data_count_width : 7
        full_threshold_assert_value : 126
        full_threshold_negate_value : 125
      output_files :
        - generate_fifo.tcl  : {file_type : tclSource}